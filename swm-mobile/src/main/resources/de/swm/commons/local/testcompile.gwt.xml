<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.1.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.1.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='kitchensink'>
	<inherits name="com.google.gwt.user.User" />
	
	<!-- Inherit GWT Mobile UI module -->
	<inherits name='de.swm.commons.mobile.swm-mobile'/>
	
	<!-- Properties -->
	<set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" />
    <!--<set-configuration-property name="CssResource.style" value="pretty"/>-->
     	
	<!-- Only compile WebKit permutation -->
	<set-property name="user.agent" value="safari"/>

	<entry-point class="de.swm.commons.local.client.GwtTestcompile" />

</module>