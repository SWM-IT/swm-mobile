<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:m="urn:import:de.swm.commons.mobile.client.widgets">
    <g:HTMLPanel>
        <m:SimpleHeaderPanel ui:field="header" leftButton="Back">
            <m:OnlyDesktopLabel>Transitions</m:OnlyDesktopLabel>
        </m:SimpleHeaderPanel>
        <m:ScrollPanel>
            <m:ListPanel ui:field="list" showArrow="false">
                <g:Label>Slide</g:Label>
                <g:Label>Slide Up</g:Label>
                <g:Label>Slide Down</g:Label>
                <g:Label>Fade</g:Label>
                <g:Label>Pop</g:Label>
                <g:Label>Swap</g:Label>
                <g:Label>Flip</g:Label>
            </m:ListPanel>
        </m:ScrollPanel>

    </g:HTMLPanel>
</ui:UiBinder> 