<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:de.swm.commons.mobile.client.widgets"
	xmlns:mt="urn:import:de.swm.commons.mobile.client.widgets.toolbar">
	<ui:style>
	</ui:style>
	<ui:with field='icon' type='de.swm.mobile.kitchensink.client.theme.icon.IconResources'/>

	<g:HTMLPanel>
		<mt:ToolbarPanel indexToSelectOnInit="0" ui:field="toolbar">
			<mt:ToolbarElement>
				<mt:ToolbarHeader headerText='Widgets' headerImage='{icon.tab0}' highlightImage='{icon.tab0selected}' />
				<mt:ToolbarContent />
			</mt:ToolbarElement>
			<mt:ToolbarElement>
				<mt:ToolbarHeader headerText='Animation' headerImage='{icon.tab1}' highlightImage='{icon.tab1selected}' />
				<mt:ToolbarContent />
			</mt:ToolbarElement>
			<mt:ToolbarElement>
				<mt:ToolbarHeader headerText='Panels' headerImage='{icon.tab3}' highlightImage='{icon.tab3selected}' />
				<mt:ToolbarContent />
			</mt:ToolbarElement>
			<mt:ToolbarElement>
				<mt:ToolbarHeader headerText='Forms' headerImage='{icon.tab3}' highlightImage='{icon.tab3selected}' />
				<mt:ToolbarContent />
			</mt:ToolbarElement>
			<mt:ToolbarElement>
				<mt:ToolbarHeader headerText='About' headerImage='{icon.tab2}' highlightImage='{icon.tab2selected}' />
				<mt:ToolbarContent />
			</mt:ToolbarElement>
        </mt:ToolbarPanel>
	</g:HTMLPanel>	
</ui:UiBinder> 