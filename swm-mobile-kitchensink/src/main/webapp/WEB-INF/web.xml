<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

   <!-- Application cache support -->
  <servlet>
    <servlet-name>ManifestServlet</servlet-name>
    <servlet-class>de.swm.gwt.linker.server.MobileHtml5ManifestServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ManifestServlet</servlet-name>
    <url-pattern>/kitchensink.manifest</url-pattern>
  </servlet-mapping>

   <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
